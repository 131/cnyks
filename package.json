{
  "name": "cnyks",
  "version": "2.4.3",
  "description": "Command line introspection tool for nodejs (interactive runner)",
  "main": "lib/",
  "scripts": {
    "build:demo": "browserify demo/main.js > demo/_main.js",
    "eslint": "eslint lib test prompt bin pty ify.js",
    "checkall": "npm run eslint",
    "coveralls": "npm install coveralls && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/html/lcov.info",
    "demo": "node bin/cnyks.js test/data/fuu.js",
    "discify": "browserify --ignore cnyks --node -p discify lib/bundle.js | discify-reporter --ir://run --source=disc/graph.json",
    "cover": "nyc --temp-directory coverage/.nyc_output --report-dir coverage/master --silent --reporter none node_modules/mocha/bin/_mocha -b test/ && nyc --temp-directory coverage/.nyc_output  report --include \"coverage/**/coverage*.json\" --report-dir coverage/html/ --reporter text-summary --reporter html --reporter lcov",
    "covesr": "nyc  --temp-directory=coverage/.nyc_output --reporter=text-summary --reporter=html --reporter=lcov  mocha",
    "test": "npm run checkall && npm run cover",
    "mocha": "node node_modules/mocha/bin/_mocha -b test/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/131/cnyks.git"
  },
  "bin": {
    "cnyks": "./bin/cnyks.js"
  },
  "keywords": [
    "command",
    "line",
    "introspection",
    "reflection"
  ],
  "author": "Francois Leurent <131.js@cloudyks.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/131/cnyks/issues"
  },
  "homepage": "https://github.com/131/cnyks",
  "dependencies": {
    "co": "^4.6.0",
    "mout": "^1.0.0",
    "nyks": "^5.11.6",
    "read-merge-28": "^1.0.7",
    "reflection-js": "^1.4.3"
  },
  "devDependencies": {
    "browserify-wrap": "^1.0.1",
    "eslint-plugin-ivs": "^1.0.7",
    "expect.js": "^0.3.1",
    "mocha": "^3.1.2",
    "nyc": "^13.0.1",
    "xterm2": "^1.2.0"
  }
}