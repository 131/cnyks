{
  "name": "cnyks",
  "version": "1.10.2",
  "description": "Command line introspection tool for nodejs (interactive runner)",
  "main": "lib/bundle.js",
  "scripts": {
    "build:demo": "browserify demo/main.js > demo/_main.js",
    "coveralls": "npm install coveralls && node ./node_modules/coveralls/bin/coveralls.js < ./coverage/html/lcov.info",
    "demo": "node bin/cnyks.js test/data/fuu.js",
    "discify": "browserify --ignore cnyks --node -p discify lib/bundle.js | discify-reporter --ir://run --source=disc/graph.json",
    "test": "node node_modules/istanbul/lib/cli.js cover --dir coverage/master --print none --report none node_modules/mocha/bin/_mocha -- -b -r mocha-plugin-co test/ && node node_modules/istanbul/lib/cli.js  report --include coverage/**/coverage.json --dir coverage/html/ text-summary html lcov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/131/cnyks.git"
  },
  "bin": {
    "cnyks": "./bin/cnyks.js"
  },
  "keywords": [
    "command",
    "line",
    "introspection",
    "reflection"
  ],
  "author": "Francois Leurent <131.js@cloudyks.org>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/131/cnyks/issues"
  },
  "homepage": "https://github.com/131/cnyks",
  "dependencies": {
    "async-co": "^1.5.0",
    "co": "^4.6.0",
    "mout": "^1.0.0",
    "nyks": "^3.6.2",
    "read": "https://github.com/131/read/tarball/master",
    "reflection-js": "^1.3.0"
  },
  "devDependencies": {
    "browserify-wrap": "^1.0.1",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^2.2.5",
    "mocha-plugin-co": "^1.0.0",
    "xterm2": "^1.2.0"
  }
}
